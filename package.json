{
  "name": "AlinmaPay",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "react-native start",
    "test": "jest",
    "docs": "jsdoc -c jsdoc.conf.json",
    "ios:dev": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayDev\" ",
    "ios:prod": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayProd\" ",
    "ios:preprod": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayPreProd\" ",
    "ios:uat": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayUat\" ",
    "ios:sit": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPaySit\" ",
    "android:prod": "react-native run-android --mode=proddebug --appId=com.alinmapay",
    "android:preprod": "react-native run-android --mode=preproddebug --appId=com.alinmapay.preprod",
    "android:dev": "react-native run-android --mode=devdebug --appId=com.alinmapay.dev",
    "android:uat": "react-native run-android --mode=uatdebug --appId=com.alinmapay.uat",
    "android:sit": "react-native run-android --mode=sitdebug --appId=com.alinmapay.sit",
    "storybook-generate": "sb-rn-get-stories"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.2.2",
    "@storybook/cli": "^8.0.8",
    "@storybook/react-native": "^7.6.18",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.0",
    "add": "^2.0.6",
    "axios": "^1.6.8",
    "crypto-js": "^4.2.0",
    "form-data": "^4.0.0",
    "i18next": "^23.10.1",
    "react": "18.2.0",
    "react-i18next": "^14.1.0",
    "react-native": "0.74.0",
    "react-native-config": "^1.5.1",
    "react-native-gesture-handler": "^2.16.0",
    "react-native-reanimated": "^3.9.0-rc.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.30.1",
    "react-native-vector-icons": "^10.0.3",
    "react-navigation": "^5.0.0",
    "react-redux": "^9.1.0",
    "redux-persist": "^6.0.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "@react-native-community/datetimepicker": "^7.7.0",
    "@react-native-community/slider": "^4.5.2",
    "@react-native/babel-preset": "0.74.81",
    "@react-native/eslint-config": "0.74.81",
    "@react-native/metro-config": "0.74.81",
    "@react-native/typescript-config": "0.74.81",
    "@storybook/addon-ondevice-actions": "^7.6.18",
    "@storybook/addon-ondevice-controls": "^7.6.18",
    "@testing-library/react-native": "^12.4.5",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.6",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-jest": "^29.6.3",
    "babel-loader": "^8.3.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "better-docs": "^2.7.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.34.1",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "prettier": "2.8.8",
    "react-dom": "18.2.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-test-renderer": "18.2.0",
    "typescript": "*"
  },
  "engines": {
    "node": ">=18"
  }
}
