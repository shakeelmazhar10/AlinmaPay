{
    "name": "AlinmaPay",
    "version": "0.0.1",
    "private": true,
    "scripts": {
      "android": "react-native run-android",
      "ios": "react-native run-ios",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "start": "react-native start",
      "test": "jest",
      "docs": "jsdoc -c jsdoc.conf.json",
      "ios:dev": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayDev\" ",
      "ios:prod": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayProd\" ",
      "ios:preprod": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayPreProd\" ",
      "ios:uat": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPayUat\" ",
      "ios:sit": "yarn react-native run-ios --mode=Debug --scheme \"AlinmaPaySit\" ",
      "android:prod": "react-native run-android --mode=proddebug --appId=com.alinmapay",
      "android:preprod": "react-native run-android --mode=preproddebug --appId=com.alinmapay.preprod",
      "android:dev": "react-native run-android --mode=devdebug --appId=com.alinmapay.dev",
      "android:uat": "react-native run-android --mode=uatdebug --appId=com.alinmapay.uat",
      "android:sit": "react-native run-android --mode=sitdebug --appId=com.alinmapay.sit",
      "storybook-generate": "sb-rn-get-stories",
      "storybook": "storybook dev -p 6006",
      "build-storybook": "storybook build"
    },
    "husky": {
      "hooks": {
        "pre-commit": "lint-staged"
      }
    },
    "lint-staged": {
      "*.{js,jsx,ts,tsx}": [
        "prettier --write",
        "eslint --fix"
      ]
    },
